<%= form_with(model: @cohort) do |form| %>
  Cohort: <%= form.text_field(:title)%><br />
  Starting: <%= form.text_field(:start_date, data:{provide:'datepicker'})%>
  Ending: <%= form.text_field(:end_date, data:{provide:'datepicker'})%>
  Course: <%= form.collection_select(:course, Course.order(:title), :id, :title)%>
<%= form.submit %>
<% end %>
