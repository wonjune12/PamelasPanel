<div class="uk-margin-medium-left">
<h1>Course</h1>
</div>
<div class="uk-card-default uk-card-body uk-width-1-2@m uk-align-center uk-animation-slide-bottom-medium">
  <%= render partial: 'course', locals: { course: @course } %>
  Course detail: <%= @course.courseinfo%><br/>
  Hours: <%= @course.hoursinclass%><br/>

  <br/> <br/>
  <button class="uk-button"><%= link_to "Edit Course", edit_course_path(@course) %> </button>
  <button class="uk-button uk-button-danger"><%= link_to "Destroy Course", course_path(@course), method: :delete %> </button>
</div>